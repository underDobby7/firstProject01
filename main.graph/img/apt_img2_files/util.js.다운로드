//숫자체크
function isNumber(ch) {
	return $.isNumeric(ch);
}

function isNull(ch) {
	var val = "";
	if(ch == null || ch == "null") {
		val = "";
	}
	else {
		val = ch;
	}
	return val;
}

//단위 한글변환
function changeHangul(val) {
   	
	var hanA = new Array("","일","이","삼","사","오","육","칠","팔","구","십");
 	var danA = new Array("", "십", "백","천","","십","백","천","","십","백","천");
 	var result = "";       	
 	var chk = false;  
 	
 	if(val != "" && val != null) {
 		if(isNumber(val)) {
 			
 			var intVal = parseInt(val, 10);
 			val = '' + intVal;
 			for(var i=0; i<val.length; i++) {
 				str = "";
 				han = hanA[val.charAt(val.length-(i+1))];
 				if(han != "") str = han+danA[i];
 				if(i < 4) {
 					if(han != "") chk = true;
 				}
 				if(i == 4) str += "억 ";
 				if(i == 8) str += "조 ";
 				result = str + result;
 			}
 		}
 		if(chk) {
 			result = result + "만";
 		}     
 	}		
 	return result;
}	

function getBobn(val) {
	var bobn = "";
	
	if(val == "" || val == null) {
		bobn = "";
	}
	else {
		if(isNumber(val)) {			
			bobn = "" + parseInt(val, 10);
			if(bobn == "0") {
				bobn = "";
			}
		}
		else {
			bobn = val;
		}
	}
	return bobn;
}

function getBubn(val) {
	var bubn = "";
	
	if(val == "" || val == null) {
		bubn = "";
	}
	else {
		if(isNumber(val)) {
			bubn = "-" + parseInt(val, 10);
			if(bubn == "-0") {
				bubn = "";
			}
		}
		else {
			bubn = "-" + val;
		}
	}
	
	return bubn;
}

function getMapPNU(val) {
	
	var vals = '';
	
	if(val == "" || val == null) {
		vals = "";
	}
	else {
		if(isNumber(val)) {
			if(parseInt(val, 10) == 0) {
				vals = '';
			}else {
				vals = '' + parseInt(val, 10);
			}			
		}
		else {
			vals = val;
		}
	}
	return vals;
}

function getFloor(val) {
	var floor = "";
	if(val != "" && val != null) {		
		if(val == "0") {
			floor = "";
		}else {
			floor = "<span style='width:24px;display:inline-block;'>(" + val + ")</span>";	
		}			
	}
	else {
		floor = "<span style='width:24px;display:inline-block;'>&nbsp;</span>";
	}
	return floor;
}

function getMoney(val) {
	var money = "";
	if(val != "0") {
		money = val;
	}
	return money;
}

function getRentMoney(val) {
	var money = "";
	if(val != "0") {
		money = "<span style='width:24px;display:inline-block;'>&nbsp;</span><br /><font color='blue'>" + val + "</font>";
	}
	return money;
}
// 단독-전월세용
function getRentMoney2(val) {
	var money = "";
	if(val != "0") {
		money = "<br /><font color='blue'>" + val + "</font>";
	}
	return money;
}

function getRightGbn(val, gbn) {
	var rightGbn = "";
	if(val == "1") {
		if(gbn == "1") {
			rightGbn = "<span style='width:24px;display:inline-block;'>&nbsp;</span>";
		}else {
			rightGbn = "";
		}
	}else if(val == "A") {
		if(gbn == "1") {
			rightGbn = "<span style='width:24px;display:inline-block;'>&nbsp;</span>";
		}else {
			rightGbn = "";
		}
	}else if(val == "2") {
		if(gbn == "1") {
			rightGbn = "<span style='width:24px;display:inline-block;'>[입]</span>";
		}else {
			rightGbn = "<br /><span style='width:24px;display:inline-block;'>[입]</span>";
		}		
	}
	else if(val == "B") {
		/*if(gbn == "1") {
			rightGbn = "<br /><span style='width:12px;'>[입(준공후)]</span>";
		}else {
			rightGbn = "<br /><span style='width:12px;display:inline-block;'>[입(준공후)]</span>";
		}*/
		if(gbn == "1") {
			rightGbn = "<br /><span style='width:12px;'>[입]</span>";
		}else {
			rightGbn = "<br /><span style='width:12px;display:inline-block;'>[입]</span>";
		}	
	}
	return rightGbn;
}

function getJibunGbn(val) {
	var jibun = "";
	if(val == "1") {
		jibun = "<span style='width:24px;display:inline-block;font-size:9px;'>지분</span>";
	}
	return jibun;
}

function getBuildYear(val) {
	var build_year = '';	
	if(val != "" && val != null) {
		build_year = val;
	}	
	return build_year;
}

function getArea(val) {
	var area = '';
	if(val != "" && val != null) {
		area = val;				
	}
	return area;
}

//금액 셋팅
function setFromAmt(fromObj, toObj, targetObj) {
	
	var fromVal = $('input[name='+fromObj+']').val();
	var toVal 	= $('input[name='+toObj+']').val();
	var fromValTxt = "";
	var toValTxt = "";
	
	if(fromVal == "" && toVal == "") {
		$('#'+targetObj).html("");
	}
	else {
		if(fromVal != "") {
			if(!isNumber(fromVal)) {
				alert("금액은 숫자만 입력가능합니다.");
				$('input[name='+fromObj+']').val('');
			}			
			else {
				fromValTxt = changeHangul(fromVal) + "";
							
				if(toVal != "") {
					toValTxt = " ~ " + changeHangul(toVal) + "";
				}
				$('#'+targetObj).html(" (" + fromValTxt + toValTxt + "원)");
			}
		}
		else {
			fromValTxt = changeHangul(fromVal) + "";
			
			if(toVal != "") {
				toValTxt = " ~ " + changeHangul(toVal) + "";
			}
			$('#'+targetObj).html(" (" + fromValTxt + toValTxt + "원)");
		}
	}
}

function setToAmt(fromObj, toObj, targetObj) {
	
	var fromVal = $('input[name='+fromObj+']').val();
	var toVal 	= $('input[name='+toObj+']').val();
	var fromValTxt = "";
	var toValTxt = "";
	
	if(fromVal == "" && toVal == "") {
		$('#'+targetObj).html("");
	}
	else {
		if(toVal != "") {
			if(!isNumber(toVal)) {
				alert("금액은 숫자만 입력가능합니다.");
				$('input[name='+toObj+']').val('');
			}			
			else {
				fromValTxt = "";
				toValTxt = " ~ " + changeHangul(toVal) + "";
				
				if(fromVal != "") {
					fromValTxt = changeHangul(fromVal) + "";
				}
				$('#'+targetObj).html(" (" + fromValTxt + toValTxt + "원)");
			}
		}
		else {
			fromValTxt = "";
			toValTxt = " ~ " + changeHangul(toVal) + "";
			
			if(fromVal != "") {
				fromValTxt = changeHangul(fromVal) + "";
			}
			$('#'+targetObj).html(" (" + fromValTxt + toValTxt + "원)");
		}
	}
}

