//select box choice
function choiceSelectBox(objNm, value) {

	$('select[name='+objNm+']').find('option').each(function() {			
 			if($(this).val() == value) {
 				$(this).prop("selected", true); 				
 			}else {
 				$(this).prop("selected", false);	      
 			}
	});
}

//select box choice - 도로명용
function choiceSelectBox1(objNm, value) {

	$('select[name='+objNm+']').find('option').each(function() {			
 			if($(this).val() == value) {
 				$(this).prop("selected", true); 				
 			}else {
 				$(this).prop("selected", false);	      
 			}
	});
	idxLoading();
	if($('select[name=chosung] option:selected').val() != "") {				
		setTimeout(function() {
			searchRoadName();	
		}, 500);
	}
}

//select box choice - 도로명용
function choiceSelectBox2(objNm, value, menuGubun) {

	$('select[name='+objNm+']').find('option').each(function() {			
 			if($(this).val() == value) {
 				$(this).prop("selected", true); 				
 			}else {
 				$(this).prop("selected", false);	      
 			}
	});
	idxLoading();
	if($('select[name=roadCode] option:selected').val() != "" && menuGubun != "C"&&menuGubun != "H") {		
		setTimeout(function() {
			searchDanji();	
		}, 500);
	}
}

// select box init
function initSelectBox(objNm) {
	
	$('select[name='+objNm+']').find('option').each(function() {
 			if($(this).val() != "") {
 				$(this).remove();	      				
 			}
	});		
}

// main용
function idxLoading() {	
	var searchBoxW = $('#idxLayer').css("width");
	var searchBoxH = $('#idxLayer').css("height");		
	$('#idxLoadingBar').css({'width' : searchBoxW, 'height': searchBoxH});
	$('#idxLoadingBar').show();	
}

function idxUnLoading() {		
	$('#idxLoadingBar').fadeOut(100);
}


// 검색페이지용
function loading() {		
	var searchBoxW = $('.search_box').css("width");
	var searchBoxH = $('.search_box').css("height");		
	$('#loadingBar').css({'width' : searchBoxW, 'height': searchBoxH});
	$('#loadingBar').show();		
}

function unloading() {	
	$('#loadingBar').fadeOut(100);
}

// 상세페이지용
function dtlloading() {	
	var searchBoxW = $('.search_box').css("width");
	var searchBoxH = $('.search_box').css("height");		
	$('#loadingBar').css({'width' : searchBoxW, 'height': searchBoxH});
	$('#loadingBar').show();	
}

function dtlUnloading() {		
	$('#loadingBar').fadeOut(100);	
}

function popClose() {
	window.close();
}

